#accordion {
    margin: 100px auto;
    width: 800px;
	margin-top: 10px;
	margin-left:320px;
	margin-right:120px;
	margin-bottom:40px;
}

#accordion li {
    list-style: none;
    width: 100%;
    margin-bottom:23px;
    background-color:white;
    padding:10px;
    border-radius: 4px; 
}

#accordion li label {
    padding:10px;
    display:flex;
    align-items:center;
    justify-content:space-between;
    font-size:18px;
	font-family: 'Baloo 2',cursive;
    font-weight:700;
    cursor:pointer;
    color:black;
}

#accordion li label span {
    transform:rotate(90deg);
    font-size:22px;
    color:#333;
}

#accordion .content p {
	padding-left: 5px;
}

#accordion label + input[type = "radio"] {
    display:none;
}

#accordion .content {
    padding: 0px;
    line-height:26px;
    max-height:0;
    overflow:hidden;
    transition:max-height 0.5s;
}

#accordion label + input[type="radio"]:checked + .content {
    max-height:400px;
}

	
